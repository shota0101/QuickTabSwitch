{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "QuickTabSwitch",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Switch tabs quickly.",
  "author": "Shota Hayashi",
  "homepage_url": "https://github.com/shota0101/QuickTabSwitch",
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "images/19.png",
    "default_popup": "browser_action.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+X",
        "windows": "Ctrl+Shift+X",
        "mac": "Ctrl+Shift+X",
        "linux": "Ctrl+Shift+X"
      }
    }
  }
}
